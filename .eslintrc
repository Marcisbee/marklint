{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true
  },
  "rules": {
    "object-curly-spacing": [
      "error",
      "always",
      {
        "arraysInObjects": true
      }
    ],
    "indent": [
      "error",
      2
    ],
    "valid-jsdoc": 0,
    "max-len": [
      "error",
      {
        "ignoreComments": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true
      }
    ]
  }
}
